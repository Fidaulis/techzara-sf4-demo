<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Julkwel Todo!{% endblock %}</title>

    {% block stylesheets %}
        <meta charset="utf-8"/>
        <link rel="icon" type="image/png" href="https://avatars1.githubusercontent.com/u/30557565?s=460&v=4">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
        <link href="{{ asset('assets/css/bootstrap.css') }}" rel="stylesheet"/>
        <link href="{{ asset('assets/css/fresh-bootstrap-table.css') }}" rel="stylesheet"/>
        <link href="{{ asset('assets/css/demo.css') }}" rel="stylesheet"/>
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
        <link href='https://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
        <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">
        <link href="https://unpkg.com/gijgo@1.9.13/css/gijgo.min.css" rel="stylesheet" type="text/css"/>
    {% endblock %}
</head>
<body>
<div class="wrapper">
    <div class="container-fluid">
        {% block body %}{% endblock %}
    </div>
</div>

{% block javascripts %}
    <script type="text/javascript" src="{{ asset('assets/js/jquery-1.11.2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/bootstrap.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/bootstrap-table.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/demo/gsdk-switch.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/demo/jquery.sharrre.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/demo/demo.js') }}"></script>
    <script src="{{ asset('js/gigjo.min.js') }}" type="text/javascript"></script>

    <script src="{{ asset('js/main.js') }}"></script>
{% endblock %}
</body>
</html>
